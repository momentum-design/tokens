      },
      "#active": {
        "background": "@color-theme-background-alert-success-active"
      }
    },
    "transparent": {
      "#normal": {
        "background": "@color-theme-background-primary-ghost"
      },
      "#hovered": {
        "background": "@color-theme-background-primary-hover"
      },
      "#active": {
        "background": "@color-theme-background-primary-active"
      }
    }
  }
