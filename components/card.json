{
  "card": {
    "comment": "Applied to Calendar Meeting Cards",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=5173%3A12345",
    "inactive": {
      "#normal": {
        "background": "@color-theme-background-secondary-normal"
      },
      "#hovered": {
        "background": "@color-theme-background-secondary-hover"
      },
      "#active": {
        "background": "@color-theme-background-secondary-active"
      }
    },
    "success": {
      "#normal": {
        "background": "@color-theme-background-alert-success-normal"
