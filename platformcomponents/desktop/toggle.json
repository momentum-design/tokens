{
  "toggle": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=4931%3A1039",
    "toggled": {
      "#normal": {
        "background": "@color-theme-control-active-normal",
        "thumb-tint": "@color-theme-text-inverted-normal",
        "border": "@color-theme-control-active-normal"
      },
      "#hovered": {
        "background": "@color-theme-control-active-hover",
        "thumb-tint": "@color-theme-text-inverted-normal",
        "border": "@color-theme-control-active-hover"
      },
      "#pressed": {
        "background": "@color-theme-control-active-pressed",
        "thumb-tint": "@color-theme-text-inverted-normal",
        "border": "@color-theme-control-active-pressed"
      },
      "#disabled": {
        "background": "@color-theme-control-active-disabled",
        "thumb-tint": "@color-theme-text-inverted-disabled",
        "border": "@color-theme-control-active-disabled"
      }
    },
    "untoggled": {
      "#normal": {
        "background": "@color-theme-common-button-secondary-normal",
        "thumb-tint": "@color-theme-text-secondary-normal",
        "border": "@color-theme-common-outline-input-normal"
      },
      "#hovered": {
        "background": "@color-theme-common-button-secondary-hover",
        "thumb-tint": "@color-theme-text-primary-normal",
        "border": "@color-theme-common-outline-input-normal"
      },
      "#pressed": {
        "background": "@color-theme-common-button-secondary-disabled",
        "thumb-tint": "@color-theme-text-primary-normal",
        "border": "@color-theme-common-outline-input-normal"
      },
      "#disabled": {
        "background": "@color-theme-control-inactive-disabled",
        "thumb-tint": "@color-theme-text-primary-disabled",
        "border": "@color-theme-outline-disabled-normal"
      }
    }
  }
}
