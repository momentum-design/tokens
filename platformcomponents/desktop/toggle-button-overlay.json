{
  "toggleButtonOverlay": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=44859%3A103949&t=p8zl3QPbotGIIFT3-1",
    "comment": "Applied to circle and pill toggle buttons",
    "inactive": {
      "#normal": {
        "background": "@color-theme-inverted-button-primary-normal",
        "border": "@color-theme-outline-button-normal",
        "text": "@color-theme-text-secondary-normal"
      },
      "#hovered": {
        "background": "@color-theme-overlay-button-secondary-hover",
        "border": "@color-theme-outline-button-normal",
        "text": "@color-theme-text-primary-normal"
      },
      "#pressed": {
        "background": "@color-theme-inverted-button-primary-pressed",
        "border": "@color-theme-outline-button-normal",
        "text": "@color-theme-text-primary-normal"
      },
      "#disabled": {
        "background": "@color-theme-inverted-button-primary-normal",
        "border": "@color-theme-outline-primary-disabled",
        "text": "@color-theme-text-primary-disabled"
      }
    },
    "active": {
      "#normal": {
        "background": "@color-theme-inverted-button-primary-pressed",
        "border": "@color-theme-outline-button-normal",
        "text": "@color-theme-text-primary-normal"
      },
      "#hovered": {
        "background": "@color-theme-inverted-button-primary-pressed",
        "border": "@color-theme-outline-button-normal",
        "text": "@color-theme-text-primary-normal"
      },
      "#pressed": {
        "background": "@color-theme-inverted-button-primary-pressed",
        "border": "@color-theme-outline-button-normal",
        "text": "@color-theme-text-primary-normal"
      },
      "#disabled": {
        "background": "@color-theme-inverted-button-primary-normal",
        "border": "@color-theme-outline-primary-disabled",
        "text": "@color-theme-text-primary-disabled"
      }
    }
  }
}
