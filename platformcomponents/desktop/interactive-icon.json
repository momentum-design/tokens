{
  "interactiveicon": {
    "comment": "Applied to icons that are clickable",
    "figma": "https://www.figma.com/file/H19CutixoN8SJGO5EXwcc8/Components---MacOS?node-id=6192%3A28111",
    "container": {
      "#normal": {
        "background": "@color-theme-button-secondary-normal"
      },
      "#hovered": {
        "background": "@color-theme-button-secondary-hover"
      },
      "#pressed": {
        "background": "@color-theme-button-secondary-pressed"
      },
      "#disabled": {
        "background": "@color-theme-button-secondary-normal"
      }
    },
    "primary": {
      "filled": {
        "#normal": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-primary-normal"
        },
        "#hovered": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-secondary-normal"
        },
        "#pressed": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-secondary-normal"
        },
        "#disabled": {
          "icon-inactive": "@color-theme-text-primary-disabled",
          "icon-active": "@color-theme-text-primary-disabled"
        }
      },
      "outline": {
        "#normal": {
          "icon-inactive": "@color-theme-text-secondary-normal",
          "icon-active": "@color-theme-text-accent-normal"
        },
        "#hovered": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-accent-hover"
        },
        "#pressed": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-accent-active"
        },
        "#disabled": {
          "icon-inactive": "@color-theme-text-primary-disabled",
          "icon-active": "@color-theme-text-primary-disabled"
        }
      }
    },
    "secondary": {
      "filled": {
        "#normal": {
          "icon-inactive": "@color-theme-text-secondary-normal",
          "icon-active": "@color-theme-text-primary-normal"
        },
        "#hovered": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-secondary-normal"
        },
        "#pressed": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "icon-active": "@color-theme-text-secondary-normal"
        },
        "#disabled": {
          "icon-inactive": "@color-theme-text-primary-disabled",
          "icon-active": "@color-theme-text-primary-disabled"
        }
      }
    },
    "tertiary": {
      "filled": {
        "#normal": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "favorite-icon-active": "@color-theme-text-warning-normal"
        },
        "#hovered": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "favorite-icon-active": "@color-theme-text-warning-hover"
        },
        "#pressed": {
          "icon-inactive": "@color-theme-text-primary-normal",
          "favorite-icon-active": "@color-theme-text-warning-active"
        },
        "#disabled": {
          "icon-inactive": "@color-theme-text-primary-disabled",
          "favorite-icon-active": "@color-theme-text-primary-disabled"
        }
      }
    },
    "quaternary": {
      "filled": {
        "#normal": {
          "icon-active": "@color-theme-text-primary-normal"
        },
        "#hovered": {
          "icon-active": "@color-theme-text-secondary-normal"
        },
        "#pressed": {
          "icon-active": "@color-theme-text-primary-normal"
        }
      }
    }
  }
}
