{
  "sharewindow": {
    "comment": "Applied to share window",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=9532%3A17790",

    "preview": {
      "#normal": {
        "active": "@theme-common-control-shareContent-active",
        "inactive": "@theme-common-control-shareContent-inactive"
      }
    },
    "border": {
      "#normal": {
        "selected": "@theme-common-control-shareContent-selected",
        "active": "@theme-common-control-shareContent-active",
        "inactive": "@theme-common-control-shareContent-inactive"
      }
    },
    "control": {
      "text": {
        "primary": "@theme-common-text-white-normal"
      },
      "button": {
        "primary": {
          "#normal": {
            "background": "@theme-common-button-primary-normal",
            "text": "@theme-common-text-black-normal",
            "border": "@theme-button-border-normal"
          },
          "#hovered": {
            "background": "@theme-common-button-primary-hover",
            "text": "@theme-common-text-black-normal",
            "border": "@theme-button-border-normal"
          },
          "#pressed": {
            "background": "@theme-common-button-primary-pressed",
            "text": "@theme-common-text-black-normal",
            "border": "@theme-button-border-normal"
          },
          "#disabled": {
            "background": "@theme-common-button-primary-normal",
            "text": "@theme-common-text-black-normal",
            "border": "@theme-button-border-normal"
          }
        },
        "secondary": {
          "#normal": {
            "background": "@theme-common-button-secondary-normal",
            "text": "@theme-common-text-white-normal",
            "border": "@theme-common-outline-button-normal"
          },
          "#hovered": {
            "background": "@theme-common-button-secondary-hover",
            "text": "@theme-common-text-white-normal",
            "border": "@theme-common-outline-button-normal"
          },
          "#pressed": {
            "background": "@theme-common-button-secondary-pressed",
            "text": "@theme-common-text-white-normal",
            "border": "@theme-common-outline-button-normal"
          },
          "#disabled": {
            "background": "@theme-common-button-secondary-pressed",
            "text": "@theme-common-text-white-disabled",
            "border": "@theme-common-outline-button-normal"
          }
        },
        "active": {
          "#normal": {
            "background": "@theme-common-button-active-normal",
            "text": "@theme-common-text-white-normal",
            "border": "@theme-common-outline-button-normal"
          },
          "#hovered": {
            "background": "@theme-common-button-active-hover",
            "text": "@theme-common-text-white-normal",
            "border": "@theme-common-outline-button-normal"
          },
          "#pressed": {
            "background": "@theme-common-button-active-pressed",
            "text": "@theme-common-text-white-normal",
            "border": "@theme-common-outline-button-normal"
          },
          "#disabled": {
            "background": "@theme-common-button-active-pressed",
            "text": "@theme-common-text-white-disabled",
            "border": "@theme-common-outline-button-normal"
          }
        }
      }
    },
    "viewwindow": {
      "#normal": {
        "active": "@theme-common-control-shareContent-active",
        "hovered": "@theme-outline-secondary-normal",
        "selected": "@theme-outline-focus-normal"
      }
    }
  }
}
