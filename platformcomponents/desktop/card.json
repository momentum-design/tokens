{
  "card": {
    "comment": "Applied to calendar card backgrounds",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=5282%3A497",
    "active": {
      "#normal": {
        "background": "@color-theme-background-alert-success-normal"
      }
    },
    "inactive": {
      "#normal": {
        "background": "@color-theme-background-secondary-normal"
      }
    },
    "cancelled": {
      "#normal": {
        "background": "@color-theme-background-primary-ghost"
      }
    },
    "disabled": {
      "#normal": {
        "border": "@color-theme-outline-disabled-normal"
      }
    }
  }
}
