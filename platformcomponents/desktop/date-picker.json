{
  "pickers": {
    "comment": "Applied to date picker.",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=51360%3A117328",
    "datePicker": {
      "month": {
        "text": {
          "#normal": "@color-theme-text-primary-normal"
        }
      },
      "weekDay": {
        "text": {
          "#normal": "@color-theme-text-secondary-normal"
        }
      },
      "date": {
        "background": {
          "#normal": "@color-theme-button-secondary-normal",
          "#hovered": "@color-theme-button-secondary-hover",
          "#pressed": "@color-theme-button-secondary-pressed",
          "#disabled": "@color-theme-button-secondary-normal",
          "#active": "@color-theme-background-accent-normal"
        },
        "text": {
          "#normal": "@color-theme-text-primary-normal",
          "#hovered": "@color-theme-text-primary-normal",
          "#pressed": "@color-theme-text-primary-normal",
          "#disabled": "@color-theme-text-primary-disabled",
          "#active": "@color-theme-common-text-white"
        },
        "border": {
          "#normal": "@color-theme-outline-button-normal"
        },
        "unreadBadge": {
          "#normal": "@color-theme-background-accent-normal",
          "#active": "@color-theme-common-text-white"
        }
      }
    },
    "dayPicker": {
      "day": {
        "inactive": {
          "#normal": {
            "background": "@color-theme-button-secondary-normal",
            "text": "@color-theme-text-primary-normal",
            "border": "@color-theme-outline-button-normal"
          },
          "#hovered": {
            "background": "@color-theme-button-secondary-hover",
            "text": "@color-theme-text-primary-normal",
            "border": "@color-theme-outline-button-normal"
          },
          "#pressed": {
            "background": "@color-theme-button-secondary-pressed",
            "text": "@color-theme-text-primary-normal",
            "border": "@color-theme-outline-button-normal"
          },
          "#disabled": {
            "background": "@color-theme-button-secondary-normal",
            "text": "@color-theme-text-primary-disabled",
            "border": "@color-theme-outline-disabled-normal"
          }
        },
        "active": {
          "#normal": {
            "background": "@color-theme-button-accent-normal",
            "text": "@color-theme-common-text-white",
            "border": "@color-theme-button-accent-normal"
          },
          "#hovered": {
            "background": "@color-theme-button-accent-hover",
            "text": "@color-theme-common-text-white",
            "border": "@color-theme-button-accent-hover"
          },
          "#pressed": {
            "background": "@color-theme-button-accent-pressed",
            "text": "@color-theme-common-text-white",
            "border": "@color-theme-button-accent-pressed"
          },
          "#disabled": {
            "background": "@color-theme-button-primary-disabled",
            "text": "@color-theme-text-primary-disabled",
            "border": "@color-theme-button-primary-disabled"
          }
        }
      }
    }
  }
}
