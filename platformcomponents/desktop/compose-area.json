{
  "compose-area": {
    "comment": "This is applied to the compose area component inside messaging tab.",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=41139%3A50175",
    "divider": {
      "default": {
        "#normal": ["@color-theme-gradientdivider-normal-0", "@color-theme-gradientdivider-normal-1"],
        "#hovered": ["@color-theme-gradientdivider-hover-0", "@color-theme-gradientdivider-hover-1"]
      },
      "announce": {
        "#normal": ["@color-theme-gradientdivider-announce-normal-0", "@color-theme-gradientdivider-announce-normal-1"],
        "#hovered": ["@color-theme-gradientdivider-announce-hover-0", "@color-theme-gradientdivider-announce-hover-1"]
      },
      "warn": {
        "#normal": ["@color-theme-gradientdivider-warning-normal-0", "@color-theme-gradientdivider-warning-normal-1"],
        "#hovered": ["@color-theme-gradientdivider-warning-hover-0", "@color-theme-gradientdivider-warning-hover-1"]
      },
      "error": {
        "#normal": ["@color-theme-gradientdivider-error-normal-0", "@color-theme-gradientdivider-error-normal-1"],
        "#hovered": ["@color-theme-gradientdivider-error-hover-0", "@color-theme-gradientdivider-error-hover-1"]
      }
    }
  }
}
