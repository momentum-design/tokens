{
  "slider": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=46518%3A104257",
    "track": {
      "indicator": {
        "#normal": {
          "background": "@color-theme-control-active-normal"
        },
        "#disabled": {
          "background": "@color-theme-control-inactive-disabled"
        }
      },
      "empty": {
        "#normal": {
          "background": "@color-theme-control-inactive-normal"
        },
        "#disabled": {
          "background": "@color-theme-control-inactive-disabled"
        }
      }
    },
    "handle": {
      "#normal": {
        "background": "@color-theme-background-solid-primary-normal",
        "border": "@color-theme-outline-primary-normal"
      },
      "#hovered": {
        "background": "@color-theme-background-solid-secondary-normal",
        "border": "@color-theme-outline-primary-normal"
      },
      "#pressed": {
        "background": "@color-theme-background-solid-tertiary-normal",
        "border": "@color-theme-outline-primary-normal"
      },
      "#disabled": {
        "background": "@color-theme-background-solid-tertiary-normal"
      }
    }
  }
}
